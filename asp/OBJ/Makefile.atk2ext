APPL_COBJS	+= cmsis_atk2_task_lifecycle.o
APPL_COBJS	+= cmsis_atk2_task_sync.o
APPL_COBJS	+= cmsis_atk2_time.o

APPL_COBJS	+= atk2os.o
APPL_COBJS	+= atk2ext_asp_config.o
APPL_COBJS	+= atk2ext_config.o

