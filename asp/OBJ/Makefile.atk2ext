ATK2EXTEND_DIR := ../../atk2ext

APPL_CFG += atk2ext.cfg
APPL_CFG += atk2ext_test.cfg

INCLUDES += -I$(ATK2EXTEND_DIR)/include
INCLUDES += -I$(ATK2EXTEND_DIR)/cmsis/include

APPLDIR	:= $(ATK2EXTEND_DIR)/cmsis/src
APPLDIR	+= $(ATK2EXTEND_DIR)/cmsis/src/core
APPLDIR	+= $(ATK2EXTEND_DIR)/cmsis/src/autosar
APPLDIR	+= $(ATK2EXTEND_DIR)/cmsis/src/sys
APPLDIR += $(ATK2EXTEND_DIR)/config
APPLDIR += $(ATK2EXTEND_DIR)/config/asp
APPLDIR += $(ATK2EXTEND_DIR)/test

APPL_COBJS	:= cmsis_generic.o
